[Unit]
Description=uWSGI Emperor Service
After = network.target

[Service]
User = usuario
Group = nginx
ExecStartPre = /home/usuario/librarysite/services/startpre.sh
ExecStart    = /home/usuario/librarysite/services/start.sh
ExecStop     = /home/usuario/librarysite/services/stop.sh
Restart = always
KillSignal = SIGQUIT
Type = notify
NotifyAccess = all

[Install]
WantedBy = multi-user.target
